{
  "expo": {
    "name": "Treasure Hunt App",
    "slug": "treasure-hunt-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "usesAppleSignIn": false
    },
    "android": {
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.joseasaelf2131.treasurehuntapp"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ebdd28de-a4d3-44f8-9d07-844dc42ab311"
      }
    }
  }
}
